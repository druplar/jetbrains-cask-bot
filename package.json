{
  "name": "jetbrains-cask-bot",
  "version": "1.5.0",
  "description": "A bot to create PRs for homebrew cask for jetbrains products",
  "main": "index.js",
  "repository": "https://github.com/leipert/jetbrains-cask-bot.git",
  "author": "Lukas Eipert <git@leipert.io>",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.4.7",
    "cache-service": "^1.3.8",
    "cache-service-file-cache": "^1.0.1",
    "child-process-promise": "^2.2.0",
    "fs-extra": "^4.0.1",
    "github": "^9.2.0",
    "lodash": "^4.17.4",
    "superagent": "^3.3.2",
    "superagent-cache-plugin": "^2.0.0"
  },
  "private": true,
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^4.0.3",
    "prettier": "^1.5.3"
  },
  "scripts": {
    "precommit": "lint-staged",
    "prettier": "./node_modules/.bin/prettier --single-quote --trailing-comma es5 --tab-width 4 --write '{lib,assets}/*.js'"
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/prettier --single-quote --trailing-comma es5 --tab-width 4 --write",
      "git add"
    ]
  }
}
